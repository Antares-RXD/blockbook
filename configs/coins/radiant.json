{
    "coin": {
        "name": "Radiant",
        "shortcut": "RXD",
        "label": "Radiant",
        "alias": "radiant"
    },
    "ports": {
      "backend_rpc": 8046,
      "backend_message_queue": 39999,
      "blockbook_internal": 9200,
      "blockbook_public": 9201
    },
    "ipc": {
      "rpc_url_template": "http://127.0.0.1:{{.Ports.BackendRPC}}",
      "rpc_user": "rpc",
      "rpc_pass": "rpc",
      "rpc_timeout": 25,
      "message_queue_binding_template": "tcp://127.0.0.1:{{.Ports.BackendMessageQueue}}"
    },
    "backend": {
      "package_name": "backend-radiant",
      "package_revision": "Antares",
      "system_user": "radiant",
      "version": "1.2.0",
      "binary_url": "https://github.com/Antares-RXD/radiant-node/releases/download/1.2.0/radiant.bin-1.2.0.tar.gz",
      "verification_type": "sha256",
      "verification_source": "bf9faa5bc08acaf0b6cd65e0037619204cb926dad51236028569f291c7bad0b8",
      "extract_command": "tar -C backend -xf",
      "exclude_files": [],
      "exec_command_template": "{{.Env.BackendInstallPath}}/{{.Coin.Alias}}/radiantd -datadir={{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend -conf={{.Env.BackendInstallPath}}/{{.Coin.Alias}}/{{.Coin.Alias}}.conf -pid=/run/{{.Coin.Alias}}/{{.Coin.Alias}}.pid",
      "logrotate_files_template": "{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/*.log",
      "postinst_script_template": "",
      "service_type": "forking",
      "service_additional_params_template": "",
      "protect_memory": true,
      "mainnet": true,
      "server_config_file": "radiant_like.conf",
      "client_config_file": "radiant_like_client.conf",
      "additional_params": {
        "daemon":1
      }
    },
    "blockbook": {
      "package_name": "blockbook-radiant",
      "system_user": "blockbook-radiant",
      "internal_binding_template": ":{{.Ports.BlockbookInternal}}",
      "public_binding_template": ":{{.Ports.BlockbookPublic}}",
      "explorer_url": "",
      "additional_params": "",
      "block_chain": {
        "parse": true,
        "subversion": "/Radiant:1.2.0/",
        "mempool_workers": 16,
        "mempool_sub_workers": 2,
        "block_addresses_to_keep": 300,
        "xpub_magic": 76067358,
        "slip44": 0,
        "additional_params": {
            "fiat_rates": "coingecko",
            "fiat_rates_vs_currencies": "AED,ARS,AUD,BDT,BHD,BMD,BRL,CAD,CHF,CLP,CNY,CZK,DKK,EUR,GBP,HKD,HUF,IDR,ILS,INR,JPY,KRW,KWD,LKR,MMK,MXN,MYR,NGN,NOK,NZD,PHP,PKR,PLN,RUB,SAR,SEK,SGD,THB,TRY,TWD,UAH,USD,VEF,VND,ZAR,BTC,ETH",
            "fiat_rates_params": "{\"coin\": \"radiant\", \"periodSeconds\": 900}"
        }
      }
    },
    "meta": {
      "package_maintainer": "Antares",
      "package_maintainer_email": "info@radiant4people.com"
    }
  }